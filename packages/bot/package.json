{
  "name": "@dca-auth/bot",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Discord bot service for DCA-Auth",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "typecheck": "tsc --noEmit",
    "deploy-commands": "tsx src/deploy-commands.ts"
  },
  "dependencies": {
    "@dca-auth/shared": "*",
    "@prisma/client": "^5.7.0",
    "discord.js": "^14.14.1",
    "ms": "^2.1.3"
  },
  "devDependencies": {
    "@types/ms": "^0.7.34",
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
